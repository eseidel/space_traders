{{#imports}}
import '{{{.}}}';
{{/imports}}

enum {{ typeName }} {
    {{#enumValues}}
    {{ enumValueName }}._('{{ enumValue }}'),
    {{/enumValues}}
    ;

    const {{typeName}}._(this.value);

    factory {{ typeName }}.fromJson(String json) {
        return {{ typeName }}.values.firstWhere(
            (value) => value.value == json,
            orElse: () =>
                throw Exception('Unknown {{ typeName }} value: $json')
        );
    }

    final String value;

    String toJson() => value;
}
